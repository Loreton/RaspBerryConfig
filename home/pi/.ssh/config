### default for all (ho notato che bisogna metterlo in fondo altrimenti entra sempre ##
Host x*
    # ProxyCommand connect-proxy -S f602250@itaca-prod.utenze.bankit.it:8081 %h %p
    ProxyCommand connect.exe -S f602250@itaca-prod.utenze.bankit.it:8081 %h %p
    # ProxyCommand nc  f602250@itaca-prod.utenze.bankit.it:8081 %h %p
    ProxyCommand "VerifyHostKeyDNS=no"

    # ProxyCommand nc -x 10.36.240.201:9946 %h %p
    IdentityFile ~/.ssh/id_rsa
    # IdentityFile d:\\tmp\\b\\id_rsa
    # IdentityFile d:\\tmp\\b\\jboss_id_rsa

    ServerAliveInterval 60
    ServerAliveCountMax 30
    TCPKeepAlive yes
    LogLevel DEBUG3
    #LocalFormard    localhost:xxx:host:yyyy
    #RemoteFormard   localhost:xxx:host:yyyy

    ForwardAgent no
    ForwardX11 no
    ForwardX11Trusted yes
    Port 22
    Protocol 2

