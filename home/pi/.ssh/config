Host alwaysdata
    User ironcat
    HostName ssh.alwaysdata.com
    ProxyCommand none

Host gitHub
    LogLevel DEBUG3
    HostName github.com
    IdentityFile ~/.ssh/id_rsa
    ProxyCommand none

Host bitbucket
    HostName bitbucket.org
    ProxyCommand none

Host cjb
    User loreto
    HostName shell.cjb.net
    ProxyCommand none

Host pilocal
    User pi
    HostName 192.168.0.22
    ProxyCommand none

Host piloreto
    ProxyCommand none
    User pi
    HostName loreton.no-ip.info
    # HostName loreto.home.dnsget.org

